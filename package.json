{
  "name": "onex-utils",
  "version": "0.0.26",
  "description": "业务工具库，提供通用的能力",
  "main": "build/index.js",
  "types": "./build",
  "browser": {
    "fs": false,
    "net": false,
    "tls": false
  },
  "files": [
    "build",
    "dist",
    "es",
    "lib"
  ],
  "scripts": {
    "commit": "git-cz",
    "tsdoc": "typedoc",
    "build": "build-scripts build",
    "test": "jest --config=jest.config.js --coverage",
    "start": "build-scripts start",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "eslint-fix": "npm run eslint -- --fix",
    "lint": "npm run eslint",
    "prepublishOnly": "npm run build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push --follow-tags"
  },
  "peerDependencies": {
    "rax": "^1.1.0"
  },
  "engines": {
    "npm": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unity-template/utils.git"
  },
  "author": {
    "name": "lg208143"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dayjs": "^1.9.6",
    "lodash.chunk": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "lodash.uniq": "^4.5.0",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@alib/build-scripts": "^0.1.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-strict-mode": "^7.12.1",
    "@iceworks/spec": "^1.0.0",
    "@microsoft/tsdoc": "^0.12.24",
    "@types/babel-core": "^6.25.6",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.14",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.defaults": "^4.2.6",
    "@types/lodash.last": "^3.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node": "^14.14.10",
    "@types/qs": "^6.9.5",
    "@types/rax": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.1.0",
    "babel-plugin-replace-ts-export-assignment": "^0.0.2",
    "babel-preset-env": "^1.7.0",
    "build-plugin-utils": "^0.4.0",
    "conventional-changelog-cli": "2.0.0",
    "current-git-branch": "^1.1.0",
    "driver-universal": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-tsdoc": "^0.2.10",
    "fs-extra": "^9.0.1",
    "git-cz": "^4.7.1",
    "husky": "^4.3.0",
    "jest": "^26.1.0",
    "jest-canvas-mock": "^2.3.0",
    "lint-staged": "^10.2.11",
    "lodash.defaults": "^4.2.0",
    "lodash.last": "^3.0.0",
    "prettier": "^2.1.2",
    "rax": "^1.1.0",
    "rax-test-renderer": "^1.0.0",
    "sinon": "^9.2.1",
    "stats-filelist": "^1.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.1",
    "tsutils": "^3.17.1",
    "type-fest": "^0.20.2",
    "typedoc": "^0.19.2",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^4.0.3"
  },
  "defaultLanguage": "zh-cn",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ]
  },
  "homepage": "https://unpkg.com/onex-utils@0.0.9/build/index.html"
